<div style="padding:15px;background:#ccc;">
  <div class="row">
    <div class="col-md-12">
      <%= notice %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>Physical Address ID:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.phyadd_id %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>Address:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.address %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>City:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.city %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>State:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.state %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>Zip Code:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.zipcode %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3>Project ID:</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%= @physicaladdress.project_id %>
    </div>
  </div>

  <div class="row" style="margin-top:15px;">
    <div class="col-md-1">
      <%= button_to 'Edit', edit_physicaladdress_path(@physicaladdress), { :class=> 'btn btn-primary', :method => 'get' } %>
    </div>
    <div class="col-md-1">
      <%= button_to 'Back', physicaladdresses_path, { :class=> 'btn btn-default', :method => 'get' } %>
    </div>
  </div>
</div>
